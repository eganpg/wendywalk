<% @center.each do |center| %>			
	<%= center.name %>
	<br>
	<%= center.phone %>
	<br>
	<%= center.street%>
	<br>
	<%=center.city%>, <%=center.state%> <%=center.zip%>
	<br>
	<%=center.info%>
	<br>
	<%=center.url%>
	<br>
	<%=center.lat%> <%=center.longit%>
	<%= image_tag "http://maps.google.com/maps/api/staticmap?size=450x300&sensor=false&zoom=12&markers=#{center.lat}%2C#{center.longit}" %>  
<% end %>
